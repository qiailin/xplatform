<!DOCTYPE xwork PUBLIC "-//OpenSymphony Group//XWork 1.1.1//EN" "http://www.opensymphony.com/xwork/xwork-1.1.1.dtd">
<xwork>
	<package name="{http://xplatform}xplatform$dict"
		extends="{http://xplatform}xplatform$framework" namespace="/dict">
		
		<action name="dictAction" class="dictAction" >
			<interceptor-ref name="authenticationStack" />
			<result name="searchCmsTbDict">/xplatform/dict/searchCmsTbDict.vm</result>
			<result name="searchCmsTbDictType">/xplatform/dict/searchCmsTbDictType.vm</result>
			<result name="toCreateDictType">/xplatform/dict/dictTypeCreate.vm</result>
			<result name="toCreateDict">/xplatform/dict/dictCreate.vm</result>
			<result name="toUpdateDictType">/xplatform/dict/dictTypeUpdate.vm</result>
			<result name="toUpdateDict">/xplatform/dict/dictUpdate.vm</result>
		</action>
	</package>
</xwork>