<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
	<head>
		<title>字典创建</title>
		<meta http-equiv="Content-Type" content="text/html; charset=gb2312" />
		#CSSM(["ext", "style"])
	</head>
	<body onbeforeunload="window.opener.closeShadeDiv();">	
		<form method="post" enctype="multipart/form-data"> 
			<table width="100%" height="100%" border="0" cellpadding="0" cellspacing="8">
				<tr>
					<td valign="top">
						<table bgColor=#ffffff width="100%" border="0" cellspacing="0" cellpadding="0">
							<tr>
						 		<td colspan="2"  class="home_title"  noWrap >
                                                                                       字典创建
			                     </td>
								 <td noWrap align="right">
									<A class=icon_but  href="#" onclick="dictCreateSave();">
										<IMG height=16
											src="$!{env.imgUrl}/image/actions/icon_add.gif" width="16" id="name"
											align=absMiddle border=0 >保存
                                        </IMG>
									</A>
								</td>
                            </tr>
							<tr>
								<td bgcolor="#FFFFFF" colspan="3">
									<input type="hidden" id="dictTypeId" name="cmsTbDict.dictTypeId" value="$!{dictTypeId}" />
									<table bgColor="#ffffff" width="100%" border="0" cellpadding="0" cellspacing="1" class="outer" >
                                        <tr>
											<td bgcolor="#FFFFFF" class="head" nowrap>
												名称<font color='red'>*</font>：
											</td>
											<td bgcolor="#FFFFFF" class="even" nowrap>
												<input type="text" id="itemName" name="cmsTbDict.itemName" />
											</td>
                                         </tr>
                                        <tr>
                                            <td bgcolor="#FFFFFF" class="head" nowrap>
												值<font color='red'>*</font>：
											</td>
											<td bgcolor="#FFFFFF" class="even" nowrap>
												<input type="text" id="itemValue" name="cmsTbDict.itemValue" />
											</td>
                                        </tr>
										<tr>
											<td bgcolor="#FFFFFF" class="head" nowrap>
												描述：
											</td>
											<td bgcolor="#FFFFFF" class="even" nowrap >
												<input type="text" name="cmsTbDict.itemDescription" id="itemDescription" />
											</td>
										</tr>
										<tr>
											<td bgcolor="#FFFFFF" class="head" nowrap>
												备注：
											</td>
											<td bgcolor="#FFFFFF" class="even" nowrap >
												 <textarea name="cmsTbDict.remark" id="remark" cols ="26" rows = "2"></textarea>
											</td>
										</tr>
									</table>
								</td>
                            </tr>
                        </table>
                    </td>
                </tr>
            </table>
		    </form>
		<iframe id="hideFrame" name="hideFrame"	src="$!{env.imgUrl}/image/space.gif" style="height: 0; width: 0"></iframe>
		
		
	</body>
	#JSM(["common", "ext", "jquery"])
    #JS_ENV_VAL()
       <script type="text/javascript" src="$!{env.imgUrl}/js/xplatform/dict/dictCreate.js"></script>

</html>