<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
	<head>
		<title>字典类型修改</title>
		<meta http-equiv="Content-Type" content="text/html; charset=gb2312" />
		#CSSM(["ext", "style"])
	</head>
		<body  onbeforeunload="window.opener.closeShadeDiv();">	
	<form  method="post"   enctype="multipart/form-data"> 
           
		<table width="100%" height="100%" border="0" cellpadding="0"
				cellspacing="8">
				<tr>
					<td valign="top">
						<table bgColor=#ffffff width="100%" border="0" cellspacing="0"
							cellpadding="0">
							<tr>
						 		<td colspan="2"  class="home_title"  noWrap >
                                                                                       字典类型修改
			                     </td>
								 <td  noWrap align="right">
									<A  class=icon_but  href="#" onclick="dictTypeUpdateSave();"><IMG height=16
											src="$!{env.imgUrl}/image/actions/icon_save.png" width="16" id="name"
											align=absMiddle border=0>保存</A>
									
								</td>
                            </tr>
							<tr>
								<td bgcolor="#FFFFFF" colspan="3">

									<table bgColor=#ffffff width="100%" border="0" cellpadding="0"
										cellspacing="1" class=outer>
                                        <tr>
											
											<td bgcolor="#FFFFFF" class="head" nowrap>
												类型名称<font color='red'>*</font>：
											</td>
											<td bgcolor="#FFFFFF" class="even" nowrap>
											  <input type="hidden" id="dictTypeId" name="cmsTbDictType.dictTypeId" value="$!{cmsTbDictType.dictTypeId}" />

												<input type="text"  id="dictTypeName"   name="cmsTbDictType.dictTypeName" value="$!{cmsTbDictType.dictTypeName}">
											</td>
                                         </tr>
                                        <tr>
                                            <td bgcolor="#FFFFFF" class="head" nowrap>
												类型值<font color='red'>*</font>：
											</td>
											
											<td bgcolor="#FFFFFF" class="even" nowrap>
												<input type="text"  id="dictTypeValue"   name="cmsTbDictType.dictTypeValue" value="$!{cmsTbDictType.dictTypeValue}">
											</td>
                                        </tr>
										<tr>
											
											<td bgcolor="#FFFFFF" class="head" nowrap>
                                                                                                                                   备注：
											</td>
										
											<td bgcolor="#FFFFFF" class="even" nowrap >
												 <textarea name="cmsTbDictType.remark" id="remark" cols ="26" rows = "2">$!{cmsTbDictType.remark}</textarea>
													
											</td>
										</tr>
                                  </table>
                                 </td>
                            </tr>
                        </table>
                    </td>
                </tr>
           </table>
	
		 </form>
			<iframe id="hideFrame" name="hideFrame"	src="$!{env.imgUrl}/image/space.gif" style="height: 0; width: 0"></iframe>
	</body>
	#JSM(["common", "ext", "jquery"])
    #JS_ENV_VAL()
     <script type="text/javascript" src="$!{env.imgUrl}/js/xplatform/dict/dictCreate.js"></script>

</html>